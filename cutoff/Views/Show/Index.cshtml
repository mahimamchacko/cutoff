@model cutoff.Models.ShowVM
@{
    ViewData["Title"] = "Cutoff Shows";
}

<div class="mx-3 row row-cols-1 row-cols-md-2 row-cols-lg-3 g-3">
    @foreach (var show in Model.Shows)
    {
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
            <img src="" class="card-img-top" alt="" />
            <div class="card h-100 bg-light text-secondary">
                <div class="card-body">
                    <h5 class="card-title text-dark">@show.ShowName</h5>
                </div>
            </div>
        </div>
    }
</div>

